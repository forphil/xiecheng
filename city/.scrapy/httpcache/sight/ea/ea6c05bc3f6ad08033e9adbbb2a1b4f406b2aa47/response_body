            INTERFACE.removeReviewType = function($that, type) {
                if (type == "on") {
                    tourist = $that.attr("data-id");
                    pagenow = 1;
                    SearchComment();
                } else if (type == "off") {
                    tourist = 0;
                    pagenow = 1;

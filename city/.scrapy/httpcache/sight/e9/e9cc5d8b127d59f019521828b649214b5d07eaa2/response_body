                            <div itemtype="//schema.org/Review" itemscope="" itemprop="review" class="comment_single">
                                <!--开始标注一条评论-->
                                    <div class="userimg">
                                        <a href="/members/380FFE51E3184F42B7455375B8AA1F63" target="_blank">
                                            <img src="https://images4.c-ctrip.com/target/Z80t0e000000786uuC26D_C_180_180.jpg" width="60" height="60"></a>
                                        <span class="ellipsis"><a itemprop="author" href="/members/380FFE51E3184F42B7455375B8AA1F63" title="南星话邮轮" target="_blank">南星话邮轮</a></span>
                                    </div>
                                <ul>
                                    <li class="title cf">
                                        <span class="f_left"><span itemtype="//schema.org/Rating" itemscope="" itemprop="reviewRating" class="starlist">
                                                                 <!--标注单条评论评分-->
                                                                 <meta content="4.0" itemprop="ratingValue">
                                                                 <!--调用星级个数,新增meta-->
                                                                 <span style="width:80%;">
                                                                     <!--20%一星-->

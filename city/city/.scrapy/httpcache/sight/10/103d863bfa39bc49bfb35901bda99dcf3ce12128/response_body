                <!-- 热门线路 -->
    <input type="hidden" id="HotVacationRouteToken">
    <script type="text/javascript">
        //
        function ShowVacationSightRouteAd() {
            // 兼容内部调用和外部接口
            var d = ($('#putDistrictId') != null && $('#putDistrictId').length > 0 && $('#putDistrictId').val())
                || "120015";

            $.ajax({
                url: '/Destinationsite/TTDSecond/SharedView/GetHotVacationRouteView',
                type: "POST",
                async: true,
                dataType: "html",
                data: { districtId: d, rank: Math.random() },
                success: function (responseText) {
                    if (responseText != "" && responseText != "\r\n") {
                        $(responseText).insertAfter('#HotVacationRouteToken');
                        ReplaceHotrouteUrl();
                    }
                },
                error: function () {
                }
            });
        }
        //设置热门线路URL
        function ReplaceHotrouteUrl() {
            var pageid = $("#page_id").val();
            var districtId = $('#putDistrictId').val();
            var freeMid = $("#freeMid").val();
            var teamMid = $("#teamMid").val();
            var putChannelName = $('#putChannelName').val();
            if (putChannelName == undefined || putChannelName == "") {
                putChannelName = "ttd";
            }
            if (freeMid == undefined || freeMid == "") {
                freeMid = "20";
            }
            if (teamMid == undefined || teamMid == "") {
                teamMid = "20";
            }
            if (pageid != "") {
                var freeCmtRef = "#ctm_ref=gs_" + "ttd" + "_" + pageid + "_" + freeMid + "_vac_{0}_" + districtId + "_undefined";
                $("#divHotFreeModule a").each(function (i) {
                    var type = $(this).attr("data-type");
                    var text = freeCmtRef.replace("{0}", type);
                    var str = $(this).attr("href") + text;
                    $(this).attr("href", str);
                });
                var teamCmtRef = "#ctm_ref=gs_" + "ttd" + "_" + pageid + "_" + teamMid + "_vac_{0}_" + districtId + "_undefined";
                $("#divHotTeamModule a").each(function (i) {
                    var type = $(this).attr("data-type");
                    var text = teamCmtRef.replace("{0}", type);
                    var str = $(this).attr("href") + text;
                    $(this).attr("href", str);
                });
            }
        }
        var asynfunc = window.asynfunc || [];
        asynfunc.push(ShowVacationSightRouteAd);
    </script>

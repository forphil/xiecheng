            var jsIsInChina = true;

            function setPoint() {
                var city = "宾川";
                var address = "鸡足山";
                var searchOptions = {
                    //搜索范围
                    location: city,
                    //搜索完成后回调， results为搜索结果
                    complete: function(results) {
                        //获取检索结果的POI数组
                        var pois = results.detail.pois;
                        if (pois && pois.length > 0) {
                            poi = pois[0];
                            getPanoInfo();
                        }
                    }
                };
                //创建检索服务实例，并将搜索设置传入其构造方法
                var ss = new qq.maps.SearchService(searchOptions);
                ss.search(address); //执行搜索
            }

            var poi;
            var getPanoInfo = function() {
                var ps = new qq.maps.PanoramaService();
                ps.getPano(poi.latLng, 150, function(data) {
                    if (data != null) {
                        $("#sightstreeturl").show();
                        $("#sightstreeturl").prev("span").show();
                    }
                });
            };

            //加载简介中图片
            function IntroducesImage() {
                $(".text_style").each(function(i) {

                    var strid = "";
                    $(this).find("img").each(function() {
                        var obj = $(this).attr("data-imgid");
                        if (obj != null && obj != "") {
                            strid += obj + ",";
                        }
                    });
                    if (strid != "") {
                        $.ajax({
                            url: "/destinationsite/SharedPage/IntroducesImage",
                            type: "post",
                            async: true,
                            data: { "writing": strid },
                            success: function(responseText) {
                                $.each(responseText, function(i, item) {
                                    $(".text_style img[data-imgid=" + item.ImageId + "]").attr("src", item.ImgageUrl);
                                });
                            },
                            error: function() {
                            }
                        });
                    }
                });
            }
